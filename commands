#Align images and cut the faces
./util/align-dlib.py ./training-images/ align outerEyesAndNose ./aligned-images/ --size 96

#Extract embedding from all the images
./batch-represent/main.lua -outDir ./generated-embeddings/ -data ./aligned-images/

#Train a classifier from generated representations
./util/classifier.py train ./generated-embeddings/

#Using the trained model check if we can recognize the person
./util/classifier.py infer generated-embeddings/classifier.pkl find-people/image.jpg

